<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent">
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_alignParentBottom="true" android:layout_height="wrap_content"
		android:layout_width="match_parent" android:background="#525552">
		<ImageView android:layout_width="wrap_content" android:src="@drawable/sipgatelogo"
			android:id="@+id/imageView1" android:layout_height="wrap_content" android:padding="15dip"></ImageView>
		<TextView android:layout_width="match_parent"
			android:paddingLeft="10dip" android:padding="10dip"
			android:layout_height="match_parent" android:id="@+id/balanceView"
			android:gravity="center_vertical"
			android:text="balanceView" android:textSize="8pt"></TextView>
	</LinearLayout>
	<ListView android:layout_height="match_parent"
		android:layout_above="@id/linearLayout1" android:id="@+id/listView1"
		android:layout_width="match_parent"></ListView>
</RelativeLayout>
